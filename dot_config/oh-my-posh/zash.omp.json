{
    "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
    "blocks": [
      {
        "alignment": "left",
        "segments": [
          {
            "foreground": "#ff5555",
            "style": "plain",
            "template": "@{{ .UserName }} ",
            "type": "session"
          },
          {
            "foreground": "#50fa7b",
            "style": "plain",
            "template": "\u279c",
            "type": "text"
          },
          {
            "foreground": "#8be9fd",
            "properties": {
              "style": "folder"
            },
            "style": "plain",
            "template": " {{ .Path }}",
            "type": "path"
          },
          {
            "foreground": "#bd93f9",
            "properties": {
              "branch_icon": ""
            },
            "style": "plain",
            "template": " <#DDB15F>git(</>{{ .HEAD }}<#DDB15F>)</>",
            "type": "git"
          },
          {
            "foreground": "#ffb86c",
            "style": "plain",
            "template": " \uf119",
            "type": "status"
          }
        ],
        "type": "prompt"
      }
    ],
    "final_space": true,
    "version": 2
  }