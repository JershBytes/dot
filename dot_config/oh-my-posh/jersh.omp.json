{
  "$schema": "https://raw.githubusercontent.com/JanDeDobbeleer/oh-my-posh/main/themes/schema.json",
  "blocks": [
    {
      "alignment": "left",
      "segments": [
        {
          "type": "os",
          "style": "plain",
          "template": " {{ if .WSL }}WSL at {{ end }}{{.Icon}} in ",
          "background": "transparent",
          "foreground": "#6d28d9",
          "text": "",
          "duration": "0s",
          "namelength": 0
        },
        {
          "foreground": "#8BE9FD",
          "properties": {
            "style": "folder"
          },
          "style": "plain",
          "template": "{{ .Folder }} ",
          "type": "path"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "right",
      "segments": [
        {
          "foreground": "#FF5555",
          "properties": {
            "fetch_status": true
          },
          "style": "plain",
          "template": " {{ .RepoName }} {{ .HEAD }}{{if .BranchStatus }} {{ .BranchStatus }}{{ end }}{{ if .Working.Changed }} \uf044 {{ .Working.String }}{{ end }}{{ if and (.Staging.Changed) (.Working.Changed) }} |{{ end }}{{ if .Staging.Changed }} \uf046 {{ .Staging.String }}{{ end }} ",
          "type": "git"
        }
      ],
      "type": "prompt"
    },
    {
      "alignment": "left",
      "newline": true,
      "segments": [
        {
          "foreground": "#FFB86C",
          "style": "plain",
          "template": " {{ .UserName }} ",
          "type": "session"
        },
        {
          "foreground": "#F8F8F2",
          "style": "plain",
          "template": "\u279c ",
          "type": "text"
        }
      ],
      "type": "prompt"
    }
  ],
  "final_space": true,
  "version": 2
}
